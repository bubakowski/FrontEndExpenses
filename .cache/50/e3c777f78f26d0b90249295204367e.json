ark:last-of-type{\r\n                font-size: 36px;\r\n            }\r\n        }\r\n        .total{\r\n            display: flex;\r\n            justify-content: start;\r\n            align-items: flex-start;\r\n            p{\r\n             margin-bottom: 5%;\r\n            }\r\n           \r\n        }\r\n        .last{\r\n            display: flex;\r\n            justify-content: end;\r\n            align-items: flex-end;\r\n            .dark{\r\n                margin-bottom: 5%;\r\n            }\r\n        }\r\n    }\r\n}\r\n","scss\\layout\\_top_bar.scss":"@use \"../abstracts/colors\" as c;\r\n@use \"../abstracts/mixins\" as m;\r\n\r\n.top_bar {\r\n\t@include m.flex(start, center);\r\n\r\n\tflex-direction: column;\r\n\tbackground-color: c.$Softred;\r\n    border-radius: 20px;\r\n\twidth: 40%;\r\n\tposition: relative;\r\n    padding: 3%;\r\n    margin-bottom: 2%;\r\n\tp {\r\n        color: c.$Verypaleorange;\r\n        margin: 1%;\r\n\t}\r\n    p:last-of-type{\r\n        font-size: 22px;\r\n        font-weight: 700;\r\n    }\r\n\timg {\r\n\t\tposition: absolute;\r\n\t\tright: 5%;\r\n\t}\r\n}\r\n","scss\\layout\\_bars.scss":"@use \"./src/scss/abstracts/colors\" as c;\r\n@use \"./src/scss/abstracts/mixins\" as m;\r\n.bars {\r\n\t@include m.flex(end, space-evenly);\r\n\tmargin-bottom: 6%;\r\n\theight: 300px;\r\n\tdisplay: flex;\r\n\tflex-direction: row;\r\n\tposition: relative;\r\n\t.bar {\r\n\t\tborder-radius: 10px;\r\n\t\twidth: 10%;\r\n\t\tbackground-color: c.$Softred;\r\n\t\tposition: relative;\r\n\t\ttransition: background ease .3s;\r\n        .amount {\r\n            display: none;\r\n            background: c.$Darkbrown;\r\n            border-radius: 10px;\r\n            color: c.$Cream;\r\n            position: absolute;\r\n            padding: 7%;\r\n            top: -55px;\r\n            left: 50%;\r\n            transform: translate(-50%, 0);\r\n            white-space: nowrap;\r\n            transition: all ease .3s;\r\n        }\r\n\t}\r\n    .bar:hover > .amount{\r\n    \r\n        display: block;\r\n    }\r\n\t.bar:hover{\r\n\t\tcursor: pointer;\r\n        background-color: hsla(10, 79%, 65%, 0.5);\r\n\t}\r\n\t.bar:hover > .amount{\r\n\t    display: block;\r\n\t}\r\n\t.week {\r\n\t\tposition: absolute;\r\n\t\twidth: 100%;\r\n\t\tbottom: -14%;\r\n\t\tdisplay: flex;\r\n\t\tflex-direction: row;\r\n\t\tjustify-content: space-evenly;\r\n\t\tp {\r\n\t\t\tcolor: c.$Mediumbrown;\r\n\t\t\twidth: 10%;\r\n\t\t\ttext-align: center;\r\n\t\t}\r\n\t}\r\n}\r\n","scss\\breakpoints\\_responsive_mobile.scss":"@media screen and(max-width:425px) {\r\n    .top_bar{\r\n        width: 80%;\r\n        margin-bottom: 5%;\r\n        padding: 5%;\r\n    }\r\n    .card{\r\n        width: 80%;\r\n        padding: 5%;\r\n        \r\n    }\r\n    .bars{\r\n        margin-bottom: 15%;\r\n        .bar{\r\n            border-radius: 5px;\r\n            .amount {\r\n                padding:15%;\r\n                z-index: 2;\r\n            }\r\n        }\r\n    }\r\n}","scss\\breakpoints\\_responsive_tablet.scss":"@media screen and(max-width:768px) {\r\n    .top_bar{\r\n        width: 80%;\r\n    }\r\n    .card{\r\n        width: 80%;\r\n    }\r\n    .bars{\r\n        margin-bottom: 10%;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"c726c51bd2465ea263c2d9bda09903fe","cacheData":{"env":{}}}